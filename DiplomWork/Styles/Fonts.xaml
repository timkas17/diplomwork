<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local ="clr-namespace:DiplomWork"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <FontFamily x:Key="UbuntuSansRegular">pack://application;,,,/Fonts/#UbuntuSans Regular</FontFamily>
    <FontFamily x:Key="UbuntuSansThin">./Fonts/#UbuntuSans Thin</FontFamily>


    <system:Double x:Key="FontSizeRegular">20</system:Double>
    <system:Double x:Key="FontSizeSmall">14</system:Double>
    <system:Double x:Key="FontSizeLarge">30</system:Double>
</ResourceDictionary>